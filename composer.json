{
  "name": "miaoxing/seq",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Seq Plugin",
  "keywords": [
    "miaoxing",
    "seq"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.0"
  },
  "require-dev": {
    "miaoxing/dev": "^6.4.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Seq\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Seq\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    },
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 6.x-dev",
      "wei/wei": "twinh/wei as 0.10.x-dev"
    }
  }
}
